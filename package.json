{
  "private": true,
  "packageManager": "pnpm@7.4.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "up": "taze major -I"
  },
  "dependencies": {
    "@vueuse/core": "^9.1.0",
    "pinia": "^2.0.17",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.0",
    "@iconify-json/carbon": "^1.1.7",
    "@types/node": "^18.6.5",
    "@unocss/reset": "^0.45.5",
    "@vitejs/plugin-vue": "^3.0.1",
    "eslint": "^8.21.0",
    "pnpm": "^7.9.0",
    "taze": "^0.7.6",
    "typescript": "^4.7.4",
    "unocss": "^0.45.5",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-vue-components": "^0.22.3",
    "vite": "^3.0.4",
    "vite-plugin-pages": "^0.25.0",
    "vue-tsc": "^0.39.5"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "@typescript-eslint/no-inferrable-types": "off",
      "vue/component-tags-order": [
        "error",{"order": ["template","script","style"]}
      ],
      "vue/attributes-order":"off",
      "prefer-const": 0,
      "no-console": 0
    }
  }
}
